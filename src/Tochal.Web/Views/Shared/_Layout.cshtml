    <script>

        $("#notifyMessageBox li").hover(function () {
            $(this).addClass("message-item-hover");

        }, function () {
            $(this).removeClass("message-item-hover");
        });

        $("#messageItem").click(function () {
            $("#messageItem > .dropdown-wrapper").css("display", "block");

        });

        $("#messageItem li").click(function () {
            var messageId = $(this).data("message-id");
            var url = `/Notification/IsRead`;
            $('#messageModal').modal();
            $.get(url, { id: messageId }, function (req) {

                if (req == "true") {
                    $(this).removeClass("unread");
                    $(this).addClass("read");
                }

            })

        });


        $(document).mouseup(function (e) {

            var container = $("#messageItem > .dropdown-wrapper");
            if (!container.is(e.target) && container.has(e.target).length === 0) {
                $("#messageItem > .dropdown-wrapper").css("display", "none");
            }
        })

        $(function () {
            setInterval(() => {
                $('button , [type="submit"]').removeAttr('disabled')
            },1000)
        })

    </script>

